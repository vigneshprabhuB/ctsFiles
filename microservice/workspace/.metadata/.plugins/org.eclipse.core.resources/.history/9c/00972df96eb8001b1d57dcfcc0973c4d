package codeLabs.entities;

public class LineItem {
	private Item item;
	private Tax tax;
	private int quantity;

	private int price;

	public Item getItem() {
		return item;
	}

	public Tax getTax() {
		return tax;
	}

	public int getQuantity() {
		return quantity;
	}

	public int getPrice() {
		return price;
	}


}
